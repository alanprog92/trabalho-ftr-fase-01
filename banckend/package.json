{
  "name": "url-shortener-api",
  "version": "1.0.0",
  "description": "API para encurtamento de URLs com PostgreSQL, Fastify e Drizzle ORM",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:migrate": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio"
  },
  "keywords": [
    "url-shortener",
    "fastify",
    "typescript",
    "postgresql",
    "drizzle"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.864.0",
    "@fastify/cors": "8.5.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@types/node": "^24.2.1",
    "dotenv": "^17.2.1",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.4",
    "fastify": "4.28.1",
    "postgres": "^3.4.7",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@fastify/type-provider-typebox": "^5.2.0",
    "nodemon": "^3.1.10"
  }
